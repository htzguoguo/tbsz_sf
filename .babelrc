{
  "plugins": [

      "transform-class-properties",
      "transform-object-rest-spread",
      "syntax-dynamic-import",
      "transform-runtime",
      "syntax-decorators",
      "transform-decorators-legacy",
      ["import", {
          "libraryName": "antd",
          "style": "css"
      }]

  ],
  "presets": [
    "babel-preset-env",
    "react",
    "flow"
  ],
  "env": {
    "start": {
      "presets": [
        "react-hmre"
      ]
    }
  }
}
